{
  "name": "dutystatemaker3",
  "version": "1.0.0",
  "description": "Duty-state maker v3 for AEGIS International.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=ItsGJK --version-string.FileDescription=CE --version-string.ProductName=\"Duty State Maker v3\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": "Gabriel Keller / gabe_jkl",
  "license": "MIT",
  "dependencies": {
    "electron": "^8.5.5"
  },
  "devDependencies": {
    "electron-icon-maker": "0.0.4",
    "electron-packager": "^14.2.1"
  }
}
